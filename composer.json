{
    "name": "veloxia/data",
    "description": "The best Veloxiadata.",
    "keywords": [
        "veloxia",
        "data",
        "vd"
    ],
    "homepage": "https://github.com/veloxiadev/data",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Viktor Svensson",
            "email": "viktor@veloxia.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0|^5.2"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "^8.0|^7.0",
        "twig/twig": "^3.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Veloxia\\Data\\": "src",
            "Veloxia\\Data\\Graph\\": "graph"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Veloxia\\Data\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Veloxia\\Data\\DataServiceProvider"
            ],
            "aliases": {
                "VD": "Veloxia\\Data\\Facades\\VD"
            }
        }
    }
}